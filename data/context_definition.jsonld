{
  "@context": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "dcterms": "http://purl.org/dc/terms/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "ex": "http://example.org/kg/entity/",
    "kgclass": "http://example.org/kg/class/",
    "kgprop": "http://example.org/kg/property/"
  },
  "@graph": [
    {
      "@id": "kgclass:FinancialMetric",
      "@type": "owl:Class",
      "rdfs:label": "Financial Metric"
    },
    {
      "@id": "kgclass:EquityMultiple",
      "@type": "owl:Class",
      "rdfs:subClassOf": { "@id": "kgclass:FinancialMetric" },
      "rdfs:label": "Equity Multiple"
    },
    {
      "@id": "kgprop:has_CEO",
      "@type": "owl:ObjectProperty",
      "rdfs:domain": { "@id": "kgclass:Company" },
      "rdfs:range": { "@id": "kgclass:Person" },
      "rdfs:label": "has CEO"
    },
    {
      "@id": "kgprop:revenue",
      "@type": "owl:DatatypeProperty",
      "rdfs:domain": { "@id": "kgclass:Company" },
      "rdfs:range": { "@id": "xsd:decimal" },
      "rdfs:label": "revenue"
    }
  ]
}
